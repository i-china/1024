prompt_context() {
  # Custom (Random emoji)
  emojis=("‚ö°Ô∏è" "üî•" "üíÄ" "üëë" "üòé" "üê∏" "üêµ" "ü¶Ñ" "üåà" "üçª" "üöÄ" "üí°" "üéâ" "üîë" "üáπüá≠" "üö¶" "üåô")
  RAND_EMOJI_N=$(( $RANDOM % ${#emojis[@]} + 1))
  prompt_segment black default "${emojis[$RAND_EMOJI_N]} "
}



